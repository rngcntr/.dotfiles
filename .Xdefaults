URxvt.saveLines: 1000
URxvt.scrollBar: false

#include  ".Xdefaults.d/solarized-dark-colors"

URxvt.secondaryScroll: true
URxvt*font: xft:Monospace:pixelsize=14, xft:Source\ Code\ Pro\ Medium:pixelsize=14:antialias=true:hinting=true
URxvt*letterSpace: -1
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: google-chrome-stable
URxvt.cursorBlink: True
URxvt.matcher.button: 1
URxvt.tabbed.saveLines: 1000
URxvt.tabbed.scrollBar: false
URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 1
URxvt.tabbed.secondaryScroll: true
URxvt.tabbed.font: xft:Bitstream Vera Sans Mono:pixelsize=12
URxvt*inheritPixmap: true 
